    "abraham/twitteroauth": "^0.7.4",
    "symfony/event-dispatcher": "^3.3",
    "silex/silex": "^2.1",
    "symfony/console": "^3.3",
    "aws/aws-sdk-php-silex": "^3.0",
    "symfony/yaml": "^3.3",
    "dflydev/doctrine-orm-service-provider": "^2.0"
