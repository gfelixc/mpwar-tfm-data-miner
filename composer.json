{
  "require": {
    "php": "^7.1",
    "ramsey/uuid": "^3.6",
    "abraham/twitteroauth": "^0.7.4",
    "symfony/event-dispatcher": "^3.3",
    "silex/silex": "^2.1",
    "symfony/console": "^3.3",
    "aws/aws-sdk-php-silex": "^3.0",
    "symfony/yaml": "^3.3",
    "alcaeus/mongo-php-adapter": "1.0.5",
    "deralex/yaml-config-service-provider" : "dev-master",
    "doctrine/instantiator": "1.0.5",
    "doctrine/mongodb-odm": "^1.1",
    "saxulum/saxulum-doctrine-mongodb-odm-provider": "^2.3",
    "antpackio/antpack-types": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:gfelixc/YamlConfigServiceProvider.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:antpackio/antpack-types.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Mpwar\\DataMiner\\": "src/DataMiner/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mpwar\\Test\\": "src/Test/"
    }
  },
  "require-dev": {
    "fzaninotto/faker": "^1.6",
    "mockery/mockery": "^0.9.9",
    "phpunit/phpunit": "^6.2",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "stable"
}
